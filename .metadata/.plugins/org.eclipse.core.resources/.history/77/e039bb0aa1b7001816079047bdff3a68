package br.senac.rn.loja.teste;

import java.util.ArrayList;

import br.senac.rn.loja.model.Sexo;

public class TestaArrayList {

	public static void main(String[] args) {
		
		ArrayList<Sexo> sexos = new ArrayList<Sexo>();
		
		Sexo s1 = new Sexo();
		s1.setId(1);
		s1.setNome("Masculino");
		s1.setSigla("M");
		
		sexos.add(s1);
		
		Sexo s2 = new Sexo();
		s2.setId(2);
		s2.setNome("Feminino");
		s2.setSigla("F");
		
		sexos.add(s2);
		
		Sexo s3 = new Sexo();
		s3.setId(2);
		s3.setNome("Não Informado");
		s3.setSigla("N");
		
		sexos.add(s3);
		
		Sexo s4 = new Sexo();
		s4.setId(2);
		s4.setNome("Não Informado");
		s4.setSigla("N");
		
		sexos.add(s4);
		
		System.out.println(sexos.get(2));
		System.out.println(sexos.contains(s4));
		
		

	}

}
